<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/inputFragment">

    <fragment
        android:id="@+id/inputFragment"
        android:name="com.example.smartmeet.ui.InputFragment"
        android:label="Input Lokasi"
        tools:layout="@layout/fragment_input">
        <action
            android:id="@+id/inputFragment_to_resultsFragment"
            app:destination="@+id/resultsFragment"/>
    </fragment>
    <fragment
        android:id="@+id/resultsFragment"
        android:name="com.example.smartmeet.ui.ResultsFragment"
        android:label="Hasil Pencarian"
        tools:layout="@layout/fragment_results" />
</navigation>